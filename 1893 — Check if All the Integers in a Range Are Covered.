from typing import List

class Solution:
    def isCovered(self, ranges: List[List[int]], left: int, right: int) -> bool:
        diff = [0] * 52   # we only care about numbers 1..50, +1 for safety

        # Build difference array
        for l, r in ranges:
            diff[l] += 1
            if r + 1 < len(diff):
                diff[r + 1] -= 1

        # Prefix sum to know coverage
        curr = 0
        for i in range(1, 51):
            curr += diff[i]
            if left <= i <= right and curr <= 0:  # not covered
                return False

        return True
